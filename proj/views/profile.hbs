<main>
        <div class="container" id="wrapper">
            <div class="popup" id="popupError">
                <div class="popup-content">
                    <div class="popup-message" id="popupErrorMessage"></div>
                    <button class="popup-btn" id="popupErrorBtn">OK</button>
                </div>
            </div>
            <div class="popup" id="popupSuccess">
                <div class="popup-content">
                    <div class="popup-message" id="popupSuccessMessage"></div>
                    <button class="popup-btn" id="popupSuccessBtn">OK</button>
                </div>
            </div>
        <form id="my-form">
            <div class="profile-pic-container">
                <img class="profpic" src="/public/images/profile.png" alt="Users Profile Picture">
                <input class="file-input" type="file" accept="image/*">
            </div>
            <div class="cover"></div>
            <div class="border">
              <div class="form-element">
                  <label>Email:</label>
                  <input id="email" type="email" value="example@example.com">
              </div>
              <hr>
              <div class="form-element">
                  <label>Name:</label>
                  <input id="name" type="text" value="David">
              </div>
              <hr>
              <div class="form-element">
                  <label>Last Name:</label>
                  <input id="lastname" type="text" value="David">
              </div>
              <hr>
             <div class="form-element">
                  <label>Password</label>
                  <input id="password" type="password" value="123456A!">
              </div>
          </div>
        
          <div class="button-container">
            <button class="btn btn-outline-primary" type="submit">Submit</button>
        </div>
        </form>
</div>
</main>